{
  "name": "manoj/toon-php-lite",
  "version": "0.4.0",
  "description": "Lite PHP implementation of TOON (Token-Oriented Object Notation)",
  "type": "library",
  "keywords": [
    "toon",
    "json",
    "llm",
    "encoder",
    "decoder",
    "serialization",
    "php"
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "ToonLite\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ToonLite\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "phpstan": "phpstan analyse"
  },
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "phpstan/phpstan": "^2.1",
    "phpstan/phpstan-strict-rules": "^2.0"
  },
  "minimum-stability": "stable"
}
